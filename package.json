{
  "name": "metacity-portal",
  "version": "1.0.0",
  "description": "",
  "main": "./server/bin/www",
  "scripts": {
    "watch": "concurrently \"grunt watch\" \"cd client/ && ng build --watch \" \"NODE_ENV=debug node ./dist/server/src/bootstrap\"",
    "build:server": "npm run check:server && grunt ts:server && tsc -p . && npm run test:server",
    "build:client": "cd client/ && ng lint --type-check && ng build && cd ../ && grunt copy:client",
    "build": "npm run build:server && npm run build:client",
    "check:server": "tslint --exclude 'server/src/**/*.d.ts' -c tslint.json 'server/src/**/*.ts' --type-check --project ./tsconfig.json ",
    "check:client": "cd client/ && ng lint --type-check && cd ../",
    "check": "npm run check:server && npm run check:client",
    "test:server": "nyc -x 'server/test/*' --reporter=html --reporter=text mocha --opts ./server/test/mocha.opts",
    "test:client": "cd client/ && ng e2e && cd ../",
    "test": "npm run test:server && npm run test:client",
    "clean:server": "rm -rf dist/server",
    "clean:client": "rm -rf dist/client client/dist",
    "clean": "npm run clean:server && npm run clean:client",
    "start": "NODE_ENV=debug node ./dist/server/src/bootstrap",
    "start:prod": "NODE_ENV=prod node ./dist/server/src/bootstrap",
    "grunt": "grunt",
    "watch:server": "mocha-typescript-watch"
  },
